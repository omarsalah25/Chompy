{
  "expo": {
    "name": "Chompy",
    "slug": "Chompy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/chompy.gif",
      "resizeMode": "contain",
      "backgroundColor": "#6ccecd"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6ccecd"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
